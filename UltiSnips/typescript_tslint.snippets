extends typescript

# @see https://palantir.github.io/tslint/usage/rule-flags/

snippet tsl "disable tslint for next line"
// tslint:disable-next-line:no-console${VISUAL}$0
endsnippet

snippet tsli "disable tslint for rest of file"
/* tslint:disable:no-console ${VISUAL}$0*/
endsnippet

snippet tslin "enable tslint for rest of file"
/* tslint:enable:no-console ${VISUAL}$0*/
endsnippet

snippet tslint "disable tslint for current line"
someCode() // tslint:disable-line
endsnippet


